31   DDRD |= (1<<PD6); {\cf20 //Define a porta Digital 6 como sa\'EDda.}\par
32   DDRD |= (1<<PD5); {\cf20 //Define a porta Digital 5 como sa\'EDda.}\par
33   \par
34   TCCR0A |= (1<<COM0A1) | (1<<WGM01) | (1<<WGM00); \par
35   TCCR0B |= (1<<CS00);\par
36   \par
37   TCCR0A |= (1<<COM0B1) | (1<<WGM01) | (1<<WGM00);\par
38   TCCR0B |= (1<<CS00); \par
39   \par
40   InitAdc();\par
41   iniciaMotor();  {\cf20 //fun\'E7\'E3o que tira o seguidor de linha da inercia}\par
42   {\cf19 while}(1)\{\par
43 \par
44     {\cf20 //Os sensores seguem a  linha preta, quando algum dos lados identificar branco o}\par
45     {\cf20 //motor do mesmo lado gira mais rapido para poder corrigir o trajeto}\par
46     \par
47     {\cf20 //ReadAdc(0) = Sensor direita}\par
48     {\cf20 //ReadAdc(1) = Sensor Esquerda}\par
49 \par

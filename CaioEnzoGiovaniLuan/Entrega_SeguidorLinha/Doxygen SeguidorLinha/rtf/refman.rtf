51     \{\par
52       OCR0A = 170;  \par
53       OCR0B = 170;\par
54     \}\par
55     {\cf19 else} {\cf19 if} ((ReadAdc(0) < 500) && (ReadAdc(1) < 500))  {\cf20 //Sensor direita e Esquerda = Branco}\par
56     \{\par
57       _delay_ms(500);\par
58       OCR0A = 0;\par
59       OCR0B = 0;\par
60     \}\par
61     {\cf19 else} {\cf19 if} (ReadAdc(0) <= 500) {\cf20 //Sensor Direito = branco | Sensor Esquerdo = preto}\par
62     \{\par
63       OCR0A = 160;\par
64       OCR0B = 20;\par
65     \}\par
66     {\cf19 else} {\cf19 if} (ReadAdc(1) <= 500) {\cf20 //Sensor Direito = preto | Sensor Esquerdo = branco}\par
67     \{\par
68       OCR0A = 20;     \par
69       OCR0B = 160;\par
70     \}\par
71   \}\par
72 \}\par
}
}
